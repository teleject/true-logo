{
  "manifest_version": 2,
  "version": "1.0",
  "name": "True Logo",

  "description": "Swap out the false Florida State logo for the real deal",
   "icons": {
    "16": "icons/fsu16.png",
    "48": "icons/fsu48.png",
    "128": "icons/fsu128.png",
    "256": "icons/fsu256.png"
  },

   "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ],

  "browser_action": {
    "default_icon": "icons/fsu48.png",
    "default_title": "True Logo"
  },

  "author": "Christopher Schmitt <schmitt@christopher.org>",

  //https://developer.chrome.com/extensions/background_pages
  "background": {
    "scripts": [
      "background.js"
    ]
  },

  //https://developer.chrome.com/extensions/match_patterns
  "permissions": [
    "activeTab",
    "*://*/*"
  ]
  }