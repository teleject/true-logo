{
  "manifest_version": 2,
  "version": "0.2.3",
  "name": "True Logo",

  "description": "Swap out the 'false' Florida State logo for the real deal. Built with love for the fans.",
   "icons": {
    "16": "icons/fsu16.png",
    "48": "icons/fsu48.png",
    "128": "icons/fsu128.png",
    "256": "icons/fsu256.png"
  },

   "permissions": [
        "tabs",
        "notifications",
        "http://*/",
        "https://*/"
    ],

  "browser_action": {
    "default_icon": "icons/fsu48.png",
    "default_title": "True Logo"
  },

  "author": "Christopher Schmitt <schmitt@christopher.org>",

  "background": {
    "scripts": [
      "background.js",
      "on.js",
      "off.js"
    ]
  },

 "permissions": [
        "tabs",
        "notifications",
        "contextMenus",
        "http://*/*",
        "https://*/*"
    ],
  "web_accessible_resources": [
    "images/*.png",
    "images/*.svg"
  ]
  }